{
  "server": {
    "host": "SERVER_HOST",
    "http": {
      "port": "SERVER_HTTP_PORT"
    },
    "https": {
      "port": "SERVER_HTTPS_PORT",
      "force": "SERVER_HTTPS_FORCE"
    },
    "public": {
      "host": "SERVER_PUBLIC_HOST",
      "http": {
        "port": "SERVER_PUBLIC_HTTP_PORT"
      },
      "https": {
        "port": "SERVER_PUBLIC_HTTPS_PORT"
      }
    }
  },
  "database": {
    "pg": {
      "host": "DB_PG_HOST",
      "port": "DB_PG_PORT",
      "db": "DB_PG_DATABASE",
      "username": "DB_PG_USERNAME",
      "password": "DB_PG_PASSWORD",
      "max": "DB_PG_MAX",
      "idleTimeout": "DB_PG_IDLE_TIMEOUT",
      "ssl": "DB_PG_USE_SSL",
      "sslOption": {
        "rejectUnauthorized": "DB_PG_SSL_REJECT_UNAUTHORIZED"
      }
    }
  },
  "cluster": {
    "server": {
      "enable": "CLUSTER_ENABLE",
      "mode": "CLUSTER_MODE",
      "count": "CLUSTER_COUNT"
    }
  },
  "custom": {
    "realProjectPathForDocker": "REAL_PROJECT_PATH_FOR_DOCKER",
    "squid": {
      "perIpInstance": "SQUID_PER_IP_INSTANCE",
      "portListener": "SQUID_PORT_LISTENER",
      "scriptApiToken": "SQUID_SCRIPT_API_TOKEN",
      "checkBlockUrl": "SQUID_CHECK_BLOCK_URL"
    },
    "timezone": {
      "locales": "TIMEZONE_LOCALES",
      "zone": "TIMEZONE_ZONE"
    },
    "docker": {
      "protocol": "DOCKER_PROTOCOL",
      "host": "DOCKER_HOST",
      "port": "DOCKER_PORT"
    },
    "jwt": {
      "type": "JWT_TYPE",
      "secret": "JWT_SECRET_KEY"
    },
    "oauth": {
      "discord": {
        "id": "OAUTH_DISCORD_CLIENT_ID",
        "secret": "OAUTH_DISCORD_CLIENT_SECRET"
      },
      "htmlPage": {
        "address": "OAUTH_HTML_PAGE_ADDRESS",
        "key": "OAUTH_HTML_PAGE_KEY_NAME"
      }
    },
    "payment": {
      "enable": "PAYMENT_ENABLE",
      "service": {
        "fastspring": {
          "auth": {
            "username": "PAYMENT_SERVICE_FASTSPRING_USERNAME",
            "password": "PAYMENT_SERVICE_FASTSPRING_PASSWORD"
          },
          "storePopupAddress": "PAYMENT_SERVICE_FASTSPRING_STORE_POPUP_ADDRESS",
          "apiAddress": "PAYMENT_SERVICE_FASTSPRING_API_ADDRESS"
        }
      }
    }
  }
}
